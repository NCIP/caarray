<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.DesignElement.impl">
  <class name="FeatureInformationImpl" table="FEATUREINFORMATION" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <many-to-one name="feature" column="FEATURE_ID" class="FeatureImpl" />
    <set name="mismatchInformation" lazy="true">
      <key column="ID" />
      <one-to-many class="MismatchInformationImpl" />
    </set>
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

