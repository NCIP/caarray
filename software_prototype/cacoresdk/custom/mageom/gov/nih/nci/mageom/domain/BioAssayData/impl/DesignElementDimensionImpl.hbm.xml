<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.BioAssayData.impl">
  <class name="gov.nih.nci.mageom.domain.BioAssayData.impl.DesignElementDimensionImpl" table="DESIGNELEMENTDIMENSION" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <discriminator>
      <column name="OJBCONCRETECLASS"/>
    </discriminator>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
    <subclass name="CompositeSequenceDimensionImpl" discriminator-value="org.biomage.BioAssayData.CompositeSequenceDimension">
      <set name="compositeSequences" table="COMPSEQDIMENSIONCOMPSEQ" lazy="true">
        <key column="COMP_SEQ_DIMENSION_ID" />
        <many-to-many class="gov.nih.nci.mageom.domain.DesignElement.impl.CompositeSequenceImpl" column="COMP_SEQ_ID" />
      </set>
    </subclass>
    <subclass name="ReporterDimensionImpl" discriminator-value="org.biomage.BioAssayData.ReporterDimension">
      <set name="reporters" table="REPORTERDIMENSIONREPORTER" lazy="true">
        <key column="REPORTER_DIMENSION_ID" />
        <many-to-many class="gov.nih.nci.mageom.domain.DesignElement.impl.ReporterImpl" column="REPORTER_ID" />
      </set>
    </subclass>
    <subclass name="FeatureDimensionImpl" discriminator-value="org.biomage.BioAssayData.FeatureDimension">
      <set name="containedFeatures" table="FEATUREDIMENSIONFEATURE" lazy="true">
        <key column="FEATURE_DIMENSION_ID" />
        <many-to-many class="gov.nih.nci.mageom.domain.DesignElement.impl.FeatureImpl" column="FEATURE_ID" />
      </set>
    </subclass>
  </class>
</hibernate-mapping>

