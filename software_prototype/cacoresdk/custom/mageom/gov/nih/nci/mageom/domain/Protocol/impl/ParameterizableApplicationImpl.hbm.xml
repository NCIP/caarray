<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.Protocol.impl">
  <class name="ParameterizableApplicationImpl" table="PARAMETERIZABLEAPPLICATION" lazy="true" polymorphism="implicit">
    <id name="id" type="long" column="ID">
      <generator class="assigned" />
    </id>
    <set name="parameterValues" lazy="true">
      <key column="ID" />
      <one-to-many class="ParameterValueImpl" />
    </set>
    <set name="auditTrail" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.AuditImpl" />
    </set>
    <set name="descriptions" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Description.impl.DescriptionImpl" />
    </set>
    <joined-subclass name="HardwareApplicationImpl" table="HARDWAREAPPLICATION" lazy="true">
      <key column="ID" />
      <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
      <set name="propertySets" lazy="true">
        <key column="ID" />
        <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
      </set>
      <property name="serialNumber" column="SERIALNUMBER" type="string" />
      <many-to-one name="hardware" column="HARDWARE_ID" class="HardwareImpl" />
    </joined-subclass>
    <joined-subclass name="SoftwareApplicationImpl" table="SOFTWAREAPPLICATION" lazy="true">
      <key column="ID" />
      <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
      <set name="propertySets" lazy="true">
        <key column="ID" />
        <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
      </set>
      <property name="releaseDate" column="RELEASEDATE" type="java.util.Date" />
      <property name="version" column="VERSION" type="string" />
      <many-to-one name="software" column="SOFTWARE_ID" class="SoftwareImpl" />
    </joined-subclass>
    <joined-subclass name="ProtocolApplicationImpl" table="PROTOCOLAPPLICATION" lazy="true">
      <key column="ID" />
      <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
      <set name="propertySets" lazy="true">
        <key column="ID" />
        <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
      </set>
      <property name="activityDate" column="ACTIVITYDATE" type="java.util.Date" />
      <set name="performers" table="PROTOCOLAPPLPERSON" lazy="true">
        <key column="PROTOCOL_APPL_ID" />
        <many-to-many class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.PersonImpl" column="PERSON_ID" />
      </set>
      <many-to-one name="protocol" column="PROTOCOL_ID" class="ProtocolImpl" />
      <set name="hardwareApplications" lazy="true">
        <key column="ID" />
        <one-to-many class="HardwareApplicationImpl" />
      </set>
      <set name="softwareApplications" lazy="true">
        <key column="ID" />
        <one-to-many class="SoftwareApplicationImpl" />
      </set>
    </joined-subclass>
  </class>
</hibernate-mapping>

