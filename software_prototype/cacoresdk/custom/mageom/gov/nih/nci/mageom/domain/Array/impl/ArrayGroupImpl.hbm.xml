<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.Array.impl">
  <class name="ArrayGroupImpl" table="ARRAYGROUP" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <property name="arraySpacingX" column="ARRAYSPACINGX" type="float" />
    <property name="arraySpacingY" column="ARRAYSPACINGY" type="float" />
    <property name="barcode" column="BARCODE" type="string" />
    <property name="length" column="LENGTH" type="float" />
    <property name="numArrays" column="NUMARRAYS" type="int" />
    <property name="orientationMark" column="ORIENTATIONMARK" type="string" />
    <property name="orientationMarkPosition" column="ORIENTATIONMARKPOSITION" type="string" />
    <property name="width" column="WIDTH" type="float" />
    <set name="arrays" lazy="true">
      <key column="ID" />
      <one-to-many class="ArrayImpl" />
    </set>
    <set name="fiducials" lazy="true">
      <key column="ID" />
      <one-to-many class="FiducialImpl" />
    </set>
    <many-to-one name="distanceUnit" column="DISTANCE_UNIT_ID" class="gov.nih.nci.mageom.domain.Measurement.impl.DistanceUnitImpl" />
    <many-to-one name="substrateType" column="SUBSTRATE_TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

