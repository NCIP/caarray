<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.Protocol.impl">
  <class name="ParameterImpl" table="PARAMETER" lazy="true" polymorphism="implicit">
    <id name="id" type="long" column="ID">
      <generator class="assigned" />
    </id>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <many-to-one name="defaultValue" class="gov.nih.nci.mageom.domain.Measurement.impl.MeasurementImpl" column="DEFAULT_VALUE_ID" />
    <many-to-one name="dataType" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" column="DATA_TYPE_ID" />
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

