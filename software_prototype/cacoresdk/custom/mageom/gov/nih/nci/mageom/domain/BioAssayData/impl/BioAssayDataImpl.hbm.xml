<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.BioAssayData.impl">
  <class name="BioAssayDataImpl" table="BIOASSAYDATA" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <discriminator>
      <column name="OJBCONCRETECLASS"/>
    </discriminator>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <set name="summaryStatistics" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
    <many-to-one name="bioDataValues" column="BIODATA_VALUES_ID" class="BioDataValuesImpl" />
    <many-to-one name="designElementDimension" column="DESIGN_ELEMENT_DIMENSION_ID" class="DesignElementDimensionImpl" />
    <many-to-one name="quantitationTypeDimension" column="QUANTITATION_TYPE_DIMENSION_ID" class="QuantitationTypeDimensionImpl" />
    <many-to-one name="bioAssayDimension" column="BIOASSAY_DIMENSION_ID" class="BioAssayDimensionImpl" />
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
    <subclass name="DerivedBioAssayDataImpl" discriminator-value="org.biomage.BioAssayData.DerivedBioAssayData">
      <many-to-one name="producerTransformation" column="PRODUCER_TRANSFORMATION_ID" class="TransformationImpl" />
    </subclass>
    <subclass name="MeasuredBioAssayDataImpl" discriminator-value="org.biomage.BioAssayData.MeasuredBioAssayData">
    </subclass>
  </class>
</hibernate-mapping>

