<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.BioAssay.impl">
  <class name="BioAssayTreatmentImpl" table="BIOASSAYTREATMENT" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <discriminator>
      <column name="OJBCONCRETECLASS"/>
    </discriminator>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <set name="protocolApplications" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Protocol.impl.ProtocolApplicationImpl" />
    </set>
    <many-to-one name="physicalBioAssay" column="PHYSICAL_BIOASSAY_ID" class="PhysicalBioAssayImpl" />
    <many-to-one name="target" column="TARGET_ID" class="PhysicalBioAssayImpl" />
    <subclass name="ImageAcquisitionImpl" discriminator-value="org.biomage.BioAssay.ImageAcquisition">
      <set name="images" lazy="true">
        <key column="ID" />
        <one-to-many class="ImageImpl" />
      </set>
    </subclass>
  </class>
</hibernate-mapping>

