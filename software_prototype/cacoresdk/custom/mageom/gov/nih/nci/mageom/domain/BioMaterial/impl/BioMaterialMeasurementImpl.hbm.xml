<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.BioMaterial.impl">
  <class name="BioMaterialMeasurementImpl" table="BIOMATERIALMEASUREMENT" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <many-to-one name="bioMaterial" column="BIOMATERIAL_ID" class="BioMaterialImpl" />
    <many-to-one name="measurement" column="MEASUREMENT_ID" class="gov.nih.nci.mageom.domain.Measurement.impl.MeasurementImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

