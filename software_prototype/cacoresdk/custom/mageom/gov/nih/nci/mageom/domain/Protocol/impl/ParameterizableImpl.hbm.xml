<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.Protocol.impl">
  <class name="ParameterizableImpl" table="PARAMETERIZABLE" lazy="true" polymorphism="implicit">
    <id name="id" type="long" column="ID">
      <generator class="assigned" />
    </id>
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
    <set name="parameterTypes" lazy="true">
      <key column="ID" />
      <one-to-many class="ParameterImpl" />
    </set>
    <joined-subclass name="HardwareImpl" table="HARDWARE" lazy="true">
      <key column="ID" />
      <property name="identifier" column="IDENTIFIER" type="string" />
      <property name="name" column="NAME" type="string" />
      <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
      <property name="make" column="MAKE" type="string" />
      <property name="model" column="MODEL" type="string" />
      <set name="softwares" lazy="true">
        <key column="ID" />
        <one-to-many class="SoftwareImpl" />
      </set>
      <set name="hardwareManufacturers" table="HARDWAREMANUFACTURERS" lazy="true">
        <key column="HARDWARE_ID" />
        <many-to-many class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.ContactImpl" column="CONTACT_ID" />
      </set>
      <many-to-one name="type" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" column="TYPE_ID" />
    </joined-subclass>
    <joined-subclass name="SoftwareImpl" table="SOFTWARE" lazy="true">
      <key column="ID" />
      <property name="identifier" column="IDENTIFIER" type="string" />
      <property name="name" column="NAME" type="string" />
      <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
      <many-to-one name="hardware" class="HardwareImpl" column="HARDWARE_ID" />
      <set name="softwareManufacturers" table="SOFTWAREMANUFACTURERS" lazy="true">
        <key column="SOFTWARE_ID" />
        <many-to-many class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.ContactImpl" column="CONTACT_ID" />
      </set>
      <many-to-one name="type" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" column="TYPE_ID" />
      <set name="softwares" table="SOFTWARE2SOFTWARE" lazy="true">
        <key column="SOFTWARE_ID" />
        <many-to-many class="SoftwareImpl" column="ASSOCIATED_SOFTWARE_ID" />
      </set>
    </joined-subclass>
    <joined-subclass name="ProtocolImpl" table="PROTOCOL" lazy="true">
      <key column="ID" />
      <property name="identifier" column="IDENTIFIER" type="string" />
      <property name="name" column="NAME" type="string" />
      <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
      <property name="text" column="TEXT" type="string" />
      <property name="title" column="TITLE" type="string" />
      <set name="hardwares" table="PROTOCOLHARDWARE" lazy="true">
        <key column="PROTOCOL_ID" />
        <many-to-many class="HardwareImpl" column="HARDWARE_ID" />
      </set>
      <set name="softwares" table="PROTOCOLSOFTWARE" lazy="true">
        <key column="PROTOCOL_ID" />
        <many-to-many class="SoftwareImpl" column="SOFTWARE_ID" />
      </set>
      <many-to-one name="type" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" column="TYPE_ID" />
    </joined-subclass>
  </class>
</hibernate-mapping>

