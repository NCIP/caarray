<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.AuditAndSecurity.impl">
  <class name="AuditImpl" table="AUDIT_TBL" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="action" column="ACTION" type="string" />
    <property name="date" column="AUDIT_DATE" type="java.util.Date" />
    <many-to-one name="performer" column="PERFORMER_ID" class="ContactImpl" />
    <set name="auditTrail" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.AuditImpl" />
    </set>
    <set name="descriptions" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Description.impl.DescriptionImpl" />
    </set>
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

