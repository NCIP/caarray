<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.AuditAndSecurity.impl">
  <class name="ContactImpl" table="CONTACT" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <set name="roles" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    </set>
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
    <joined-subclass name="PersonImpl" table="PERSON" lazy="true">
      <key column="ID" />
      <property name="identifier" column="IDENTIFIER" type="string" />
      <property name="name" column="NAME" type="string" />
      <property name="address" column="ADDRESS" type="string" />
      <property name="email" column="EMAIL" type="string" />
      <property name="fax" column="FAX" type="string" />
      <property name="phone" column="PHONE" type="string" />
      <property name="tollFreePhone" column="TOLLFREEPHONE" type="string" />
      <property name="URI" column="URI" type="string" />
      <property name="firstName" column="FIRSTNAME" type="string" />
      <property name="lastName" column="LASTNAME" type="string" />
      <property name="midInitials" column="MIDINITIALS" type="string" />
      <many-to-one name="affiliation" column="AFFILIATION_ID" class="OrganizationImpl" />
      <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    </joined-subclass>
    <joined-subclass name="OrganizationImpl" table="ORGANIZATION" lazy="true">
      <key column="ID" />
      <property name="identifier" column="IDENTIFIER" type="string" />
      <property name="name" column="NAME" type="string" />
      <property name="address" column="ADDRESS" type="string" />
      <property name="email" column="EMAIL" type="string" />
      <property name="fax" column="FAX" type="string" />
      <property name="phone" column="PHONE" type="string" />
      <property name="tollFreePhone" column="TOLLFREEPHONE" type="string" />
      <property name="URI" column="URI" type="string" />
      <many-to-one name="parent" column="PARENT_ID" class="OrganizationImpl" />
      <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    </joined-subclass>
  </class>
</hibernate-mapping>

