<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.BioMaterial.impl">
  <class name="TreatmentImpl" table="TREATMENT" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <set name="protocolApplications" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Protocol.impl.ProtocolApplicationImpl" />
    </set>
    <many-to-one name="action" column="ACTION_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <many-to-one name="actionMeasurement" column="ACTION_MEASUREMENT_ID" class="gov.nih.nci.mageom.domain.Measurement.impl.MeasurementImpl" />
    <set name="compoundMeasurements" lazy="true">
      <key column="ID" />
      <one-to-many class="CompoundMeasurementImpl" />
    </set>
    <set name="sourceBioMaterialMeasurements" lazy="true">
      <key column="ID" />
      <one-to-many class="BioMaterialMeasurementImpl" />
    </set>
  </class>
</hibernate-mapping>

