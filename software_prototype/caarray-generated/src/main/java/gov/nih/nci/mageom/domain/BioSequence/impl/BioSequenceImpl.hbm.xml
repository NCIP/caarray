<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.BioSequence.impl">
  <class name="BioSequenceImpl" table="BIOSEQUENCE" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <property name="isApproximateLength" column="ISAPPROXIMATELENGTH" type="boolean" />
    <property name="isCircular" column="ISCIRCULAR" type="boolean" />
    <property name="length" column="LENGTH" type="int" />
    <property name="sequence" column="SEQUENCE" type="string" />
    <set name="seqFeatures" lazy="true">
      <key column="ID" />
      <one-to-many class="SeqFeatureImpl" />
    </set>
    <set name="sequenceDatabases" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Description.impl.DatabaseEntryImpl" />
    </set>
    <set name="ontologyEntries" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    </set>
    <many-to-one name="polymerType" column="POLYMER_TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <many-to-one name="type" column="TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <many-to-one name="species" column="SPECIES_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

