<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.Protocol.impl">
  <class name="ParameterValueImpl" table="PARAMETERVALUE" lazy="true" polymorphism="implicit">
    <id name="id" type="long" column="ID">
      <generator class="assigned" />
    </id>
    <property name="value" column="VALUE" type="string" />
    <many-to-one name="parameterType" column="PARAMETER_TYPE_ID" class="ParameterImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

