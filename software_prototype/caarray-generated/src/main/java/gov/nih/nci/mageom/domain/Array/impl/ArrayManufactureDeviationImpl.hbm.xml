<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.Array.impl">
  <class name="ArrayManufactureDeviationImpl" table="ARRAYMANUFACTUREDEVIATION" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <set name="adjustments" lazy="true">
      <key column="ID" />
      <one-to-many class="ZoneDefectImpl" />
    </set>
    <set name="featureDefects" lazy="true">
      <key column="ID" />
      <one-to-many class="FeatureDefectImpl" />
    </set>
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

