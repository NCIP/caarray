<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.Experiment.impl">
  <class name="ExperimentDesignImpl" table="EXPERIMENTDESIGN" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <many-to-one name="replicateDescription" column="REPLICATE_DESCRIPTION_ID" class="gov.nih.nci.mageom.domain.Description.impl.DescriptionImpl" />
    <many-to-one name="qualityControlDescription" column="QUALITY_CONTROL_DESCRIPTION_ID" class="gov.nih.nci.mageom.domain.Description.impl.DescriptionImpl" />
    <many-to-one name="normalizationDescription" column="NORMALIZATION_DESCRIPTION_ID" class="gov.nih.nci.mageom.domain.Description.impl.DescriptionImpl" />
    <set name="topLevelBioAssays" table="TOPLEVELBIOASSAYS" lazy="true">
      <key column="EXPERIMENT_DESIGN_ID" />
      <many-to-many class="gov.nih.nci.mageom.domain.BioAssay.impl.BioAssayImpl" column="BIOASSAY_ID" />
    </set>
    <set name="experimentalFactors" lazy="true">
      <key column="ID" />
      <one-to-many class="ExperimentalFactorImpl" />
    </set>
    <set name="types" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    </set>
    <set name="auditTrail" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.AuditImpl" />
    </set>
    <set name="descriptions" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Description.impl.DescriptionImpl" />
    </set>
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

