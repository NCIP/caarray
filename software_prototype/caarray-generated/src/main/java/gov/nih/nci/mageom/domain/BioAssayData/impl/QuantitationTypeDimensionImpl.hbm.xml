<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.BioAssayData.impl">
  <class name="QuantitationTypeDimensionImpl" table="QUANTITATIONTYPEDIMENSION" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <set name="quantitationTypes" table="QUANTTYPEDIMENSIONQUANTTYPE" lazy="true">
      <key column="QUANTITATION_TYPE_DIMENSION_ID" />
      <many-to-many class="gov.nih.nci.mageom.domain.QuantitationType.impl.QuantitationTypeImpl" column="QUANTITATION_TYPE_ID" />
    </set>
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

