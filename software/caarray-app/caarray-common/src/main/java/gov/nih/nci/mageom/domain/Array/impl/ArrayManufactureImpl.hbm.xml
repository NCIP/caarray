<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.Array.impl">
  <class name="ArrayManufactureImpl" table="ARRAYMANUFACTURE" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <property name="manufacturingDate" column="MANUFACTURINGDATE" type="java.util.Date" />
    <property name="tolerance" column="TOLERANCE" type="float" />
    <set name="arrayManufacturers" table="ARRAYMANUFACTURER" lazy="true">
      <key column="ARRAY_MANUFACTURE_ID" />
      <many-to-many class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.ContactImpl" column="CONTACT_ID" />
    </set>
    <set name="qualityControlStatistics" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
    <set name="arrays" lazy="true">
      <key column="ID" />
      <one-to-many class="ArrayImpl" />
    </set>
    <set name="featureLIMSs" lazy="true">
      <key column="ID" />
      <one-to-many class="ManufactureLIMSBiomaterialImpl" />
    </set>
    <set name="protocolApplications" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Protocol.impl.ProtocolApplicationImpl" />
    </set>
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

