<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.Array.impl">
  <class name="FeatureDefectImpl" table="FEATUREDEFECT" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <many-to-one name="feature" column="FEATURE_ID" class="gov.nih.nci.mageom.domain.DesignElement.impl.FeatureImpl" />
    <many-to-one name="defectType" column="DEFECT_TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <many-to-one name="positionDelta" column="POSITION_DELTA_ID" class="PositionDeltaImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

