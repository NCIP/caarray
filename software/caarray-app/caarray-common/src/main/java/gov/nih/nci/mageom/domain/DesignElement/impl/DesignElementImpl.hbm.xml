<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.DesignElement.impl">
  <class name="FeatureImpl" table="FEATURE" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <many-to-one name="controlType" column="CONTROL_TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <many-to-one name="featureGroup" column="FEATURE_GROUP_ID" class="gov.nih.nci.mageom.domain.ArrayDesign.impl.FeatureGroupImpl" />
    <many-to-one name="position" column="POSITION_ID" class="PositionImpl" />
    <many-to-one name="featureLocation" column="FEATURE_LOCATION_ID" class="FeatureLocationImpl" />
    <many-to-one name="zone" column="ZONE_ID" class="gov.nih.nci.mageom.domain.ArrayDesign.impl.ZoneImpl" />
    <set name="controlFeatures" table="FEATURECONTROLFEATURE" lazy="true">
      <key column="FEATURE_ID" />
      <many-to-many class="FeatureImpl" column="CONTROL_FEATURE_ID" />
    </set>
    <set name="controlledFeatures" table="FEATURECONTROLLEDFEATURE" lazy="true">
      <key column="FEATURE_ID" />
      <many-to-many class="FeatureImpl" column="CONTROLLED_FEATURE_ID" />
    </set>
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <!-- 
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
    -->
  </class>
  <class name="ReporterImpl" table="REPORTER" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <many-to-one name="controlType" column="CONTROL_TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <set name="failTypes" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    </set>
    <set name="immobilizedCharacteristics" table="IMMOBILIZEDCHARACTERISTICS" lazy="true">
      <key column="REPORTER_ID" />
      <many-to-many class="gov.nih.nci.mageom.domain.BioSequence.impl.BioSequenceImpl" column="BIOSEQUENCE_ID" />
    </set>
    <set name="featureReporterMaps" lazy="true">
      <key column="ID" />
      <one-to-many class="FeatureReporterMapImpl" />
    </set>
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <!--
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
    -->
  </class>
  <class name="CompositeSequenceImpl" table="COMPOSITESEQUENCE" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <many-to-one name="controlType" column="CONTROL_TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <set name="biologicalCharacteristics" table="BIOLOGICALCHARACTERISTICS" lazy="true">
      <key column="COMPOSITE_SEQUENCE_ID" />
      <many-to-many class="gov.nih.nci.mageom.domain.BioSequence.impl.BioSequenceImpl" column="BIOSEQUENCE_ID" />
    </set>
    <set name="reporterCompositeMaps" lazy="true">
      <key column="ID" />
      <one-to-many class="ReporterCompositeMapImpl" />
    </set>
    <set name="compositeCompositeMaps" lazy="true">
      <key column="ID" />
      <one-to-many class="CompositeCompositeMapImpl" />
    </set>
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

