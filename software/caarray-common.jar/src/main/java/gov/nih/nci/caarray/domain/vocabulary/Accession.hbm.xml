<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gov.nih.nci.caarray.domain.vocabulary.Accession" table="ACCESSION" polymorphism="implicit">
        <id name="id" column="ID" type="long">
            <generator class="assigned"/>
        </id>
        <property name="url" column="URL" type="string" />
        <property name="value" column="VALUE" type="string" />
        <many-to-one name="source" column="SOURCE_ID" class="gov.nih.nci.caarray.domain.vocabulary.Source" cascade="save-update" />
    </class>
</hibernate-mapping>
