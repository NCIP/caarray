<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gov.nih.nci.caarray.domain.protocol.Protocol" table="PROTOCOL" polymorphism="implicit">
        <id name="id" column="ID" type="long">
            <generator class="assigned"/>
        </id>
        <property name="accession" column="ACCESSION" type="string" />
        <property name="name" column="NAME" type="string" />
        <property name="text" column="TEXT" type="string" />
        <property name="title" column="TITLE" type="string" />
        <property name="url" column="URL" type="string" />
        <many-to-one name="type" column="TYPE_ID" class="gov.nih.nci.caarray.domain.vocabulary.Term" />
        <set name="parameters" lazy="true">
            <key column="ID" />
            <one-to-many class="gov.nih.nci.caarray.domain.protocol.Parameter" />
        </set>
    </class>
</hibernate-mapping>
