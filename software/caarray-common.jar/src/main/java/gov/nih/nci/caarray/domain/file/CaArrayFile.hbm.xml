<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gov.nih.nci.caarray.domain.file.CaArrayFile" table="CAARRAYFILE" polymorphism="implicit">
        <id name="id" column="ID" type="long">
            <generator class="assigned"/>
        </id>
        <property name="path" column="PATH" type="string" />
        <property name="type" column="TYPE" type="gov.nih.nci.caarray.dao.customtype.FileTypeUserType" />
    </class>
</hibernate-mapping>
