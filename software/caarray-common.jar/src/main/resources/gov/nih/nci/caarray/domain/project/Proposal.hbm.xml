<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gov.nih.nci.caarray.domain.project.Proposal" table="PROPOSAL" polymorphism="implicit">
        <id name="id" column="ID" type="long">
            <generator class="assigned"/>
        </id>
        <many-to-one name="project" column="PROJECT_ID" class="gov.nih.nci.caarray.domain.project.Project"
                     not-null="true" cascade="save-update" />
    </class>
</hibernate-mapping>
