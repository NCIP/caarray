<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gov.nih.nci.caarray.domain.protocol.Parameter" table="PARAMETER" polymorphism="implicit">
        <id name="id" column="ID" type="long">
            <generator class="assigned"/>
        </id>
        <property name="lsidAuthority" column="LSIDAUTHORITY" type="string" />
        <property name="lsidNamespace" column="LSIDNAMESPACE" type="string" />
        <property name="lsidObjectId" column="LSIDOBJECTID" type="string" />
        <property name="name" column="NAME" type="string" />
        <many-to-one name="defaultValue" column="DEFAULT_VALUE_ID" class="gov.nih.nci.caarray.domain.protocol.ParameterValue" lazy="false" />
    </class>
</hibernate-mapping>
