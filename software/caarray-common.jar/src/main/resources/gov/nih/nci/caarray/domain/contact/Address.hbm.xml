<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gov.nih.nci.caarray.domain.contact.Address" table="ADDRESS" polymorphism="implicit">
        <id name="id" column="ID" type="long">
            <generator class="assigned"/>
        </id>
        <property name="city" column="CITY" type="string" />
        <property name="state" column="STATE" type="string" />
        <property name="streetAddress1" column="STREETADDRESS1" type="string" />
	<property name="streetAddress2" column="STREETADDRESS2" type="string" />
        <property name="zipCode" column="ZIPCODE" type="string" />
    </class>
</hibernate-mapping>
