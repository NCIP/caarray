<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gov.nih.nci.caarray.domain.file.CaArrayFile" table="CAARRAYFILE" polymorphism="implicit">
        <id name="id" column="ID" type="long">
            <generator class="native"/>
        </id>
        <property name="lsidAuthority" column="LSIDAUTHORITY" type="string" />
        <property name="lsidNamespace" column="LSIDNAMESPACE" type="string" />
        <property name="lsidObjectId" column="LSIDOBJECTID" type="string" />
        <property name="path" column="PATH" type="string" />
        <property name="type" column="TYPE" type="gov.nih.nci.caarray.dao.customtype.FileTypeUserType" />
        <many-to-one name="project" column="PROJECT_ID" class="gov.nih.nci.caarray.domain.project.Project"
                     insert="false" update="false" cascade="none"/>
    </class>
</hibernate-mapping>
