<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gov.nih.nci.caarray.domain.publication.Publication" table="PUBLICATION" polymorphism="implicit">
        <id name="id" column="ID" type="long">
            <generator class="assigned"/>
        </id>
        <property name="lsidAuthority" column="LSIDAUTHORITY" type="string" />
        <property name="lsidNamespace" column="LSIDNAMESPACE" type="string" />
        <property name="lsidObjectId" column="LSIDOBJECTID" type="string" />
        <property name="title" column="TITLE" type="string" />
        <property name="authors" column="AUTHORS" type="string" />
        <property name="doi" column="DOI" type="string" />
        <property name="pubMedId" column="PUBMEDID" type="string" />
        <many-to-one name="status" column="STATUS_ID" class="gov.nih.nci.caarray.domain.vocabulary.Term" lazy="false" cascade="save-update" />
    </class>
</hibernate-mapping>
