<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.BioAssayData.impl">
  <class name="BioAssayMappingImpl" table="BIOASSAYMAPPING" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <set name="bioAssayMaps" table="BIOASSAYMAPS" lazy="true">
      <key column="BIOASSAY_MAPPING_ID" />
      <many-to-many class="BioAssayMapImpl" column="BIOASSAY_MAP_ID" />
    </set>
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

