<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.ArrayDesign.impl">
  <class name="ArrayDesignImpl" table="ARRAYDESIGN" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <discriminator>
      <column name="OJBCONCRETECLASS"/>
    </discriminator>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <property name="numberOfFeatures" column="NUMBEROFFEATURES" type="int" />
    <property name="version" column="VERSION" type="string" />
    <set name="compositeGroups" table="ARRAYDESIGNCOMPOSITEGRP" lazy="true">
      <key column="ARRAY_DESIGN_ID" />
      <many-to-many class="CompositeGroupImpl" column="COMPOSITE_GROUP_ID" />
    </set>
    <set name="reporterGroups" table="ARRAYDESIGNREPORTERGRP" lazy="true">
      <key column="ARRAY_DESIGN_ID" />
      <many-to-many class="ReporterGroupImpl" column="REPORTER_GROUP_ID" />
    </set>
    <set name="featureGroups" table="ARRAYDESIGNFEATUREGRP" lazy="true">
      <key column="ARRAY_DESIGN_ID" />
      <many-to-many class="FeatureGroupImpl" column="FEATURE_GROUP_ID" />
    </set>
    <set name="designProviders" table="ARRAYDESIGNPROVIDER" lazy="true">
      <key column="ARRAY_DESIGN_ID" />
      <many-to-many class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.ContactImpl" column="CONTACT_ID" />
    </set>
    <set name="protocolApplications" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Protocol.impl.ProtocolApplicationImpl" />
    </set>
    <set name="auditTrail" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.AuditImpl" />
    </set>
    <set name="descriptions" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Description.impl.DescriptionImpl" />
    </set>
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
    <subclass name="PhysicalArrayDesignImpl" discriminator-value="org.biomage.ArrayDesign.PhysicalArrayDesign">
      <set name="zoneGroups" lazy="true">
        <key column="ID" />
        <one-to-many class="ZoneGroupImpl" />
      </set>
      <many-to-one name="surfaceType" column="SURFACE_TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    </subclass>
  </class>
</hibernate-mapping>

