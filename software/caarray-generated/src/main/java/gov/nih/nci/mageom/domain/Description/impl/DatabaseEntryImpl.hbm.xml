<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.Description.impl">
  <class name="DatabaseEntryImpl" table="DATABASEENTRY" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="accession" column="ACCESSION" type="string" />
    <property name="accessionVersion" column="ACCESSIONVERSION" type="string" />
    <property name="URI" column="URI" type="string" />
    <many-to-one name="database" column="DATABASE_ID" class="DatabaseImpl" />
    <many-to-one name="type" column="TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

