<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.QuantitationType.impl">
  <class name="QuantitationTypeImpl" table="QUANTITATIONTYPE" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <discriminator>
      <column name="OJBCONCRETECLASS"/>
    </discriminator>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <property name="isBackground" column="ISBACKGROUND" type="boolean" />
    <many-to-one name="scale" column="SCALE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <many-to-one name="dataType" column="DATA_TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <many-to-one name="channel" column="CHANNEL_ID" class="gov.nih.nci.mageom.domain.BioAssay.impl.ChannelImpl" />
    <set name="quantitationTypeMaps" table="QUANTITATIONTYPEMAPSOURCES" lazy="true">
      <key column="QUANTITATION_TYPE_ID" />
      <many-to-many class="gov.nih.nci.mageom.domain.BioAssayData.impl.QuantitationTypeMapImpl" column="QUANTITATION_TYPE_MAP_ID" />
    </set>
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
    <subclass name="PresentAbsentImpl" discriminator-value="org.biomage.QuantitationType.PresentAbsent" />
    <subclass name="FailedImpl" discriminator-value="org.biomage.QuantitationType.Failed" />
    <subclass name="MeasuredSignalImpl" discriminator-value="org.biomage.QuantitationType.MeasuredSignal" />
    <subclass name="DerivedSignalImpl" discriminator-value="org.biomage.QuantitationType.DerivedSignal" />
    <subclass name="RatioImpl" discriminator-value="org.biomage.QuantitationType.Ratio" />
    <subclass name="SpecializedQuantitationTypeImpl" discriminator-value="org.biomage.QuantitationType.SpecializedQuantitationType" />
    <subclass name="PValueImpl" discriminator-value="org.biomage.QuantitationType.PValue" >
      <many-to-one name="targetQuantitationType" column="QUANTITATION_TYPE_ID" class="QuantitationTypeImpl" />
    </subclass>
    <subclass name="ErrorImpl" discriminator-value="org.biomage.QuantitationType.Error" >
      <many-to-one name="targetQuantitationType" column="QUANTITATION_TYPE_ID" class="QuantitationTypeImpl" />
    </subclass>
    <subclass name="ExpectedValueImpl" discriminator-value="org.biomage.QuantitationType.ExpectedValue" >
      <many-to-one name="targetQuantitationType" column="QUANTITATION_TYPE_ID" class="QuantitationTypeImpl" />
    </subclass>
  </class>
</hibernate-mapping>

