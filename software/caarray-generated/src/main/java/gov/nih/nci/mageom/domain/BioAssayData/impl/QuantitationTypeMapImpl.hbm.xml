<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.BioAssayData.impl">
  <class name="QuantitationTypeMapImpl" table="QUANTITATIONTYPEMAP" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <set name="protocolApplications" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Protocol.impl.ProtocolApplicationImpl" />
    </set>
    <many-to-one name="targetQuantitationType" column="QUANTITATION_TYPE_ID" class="gov.nih.nci.mageom.domain.QuantitationType.impl.QuantitationTypeImpl" />
    <set name="sourcesQuantitationType" table="QUANTITATIONTYPEMAPSOURCES" lazy="true">
      <key column="QUANTITATION_TYPE_MAP_ID" />
      <many-to-many class="gov.nih.nci.mageom.domain.QuantitationType.impl.QuantitationTypeImpl" column="QUANTITATION_TYPE_ID" />
    </set>
  </class>
</hibernate-mapping>

