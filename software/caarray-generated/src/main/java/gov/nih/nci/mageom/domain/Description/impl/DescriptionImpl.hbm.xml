<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.Description.impl">
  <class name="DescriptionImpl" table="DESCRIPTION" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="text" column="TEXT" type="string" />
    <property name="URI" column="URI" type="string" />
    <set name="bibliographicReferences" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.BQS.impl.BibliographicReferenceImpl" />
    </set>
    <set name="annotations" lazy="true">
      <key column="ID" />
      <one-to-many class="OntologyEntryImpl" />
    </set>
    <many-to-one name="externalReference" column="EXTERNAL_REFERENCE_ID" class="ExternalReferenceImpl" />
    <set name="databaseReferences" lazy="true">
      <key column="ID" />
      <one-to-many class="DatabaseEntryImpl" />
    </set>
    <set name="auditTrail" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.AuditImpl" />
    </set>
    <set name="descriptions" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.Description.impl.DescriptionImpl" />
    </set>
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

