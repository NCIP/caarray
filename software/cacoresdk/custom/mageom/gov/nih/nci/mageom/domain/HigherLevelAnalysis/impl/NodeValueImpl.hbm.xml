<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.HigherLevelAnalysis.impl">
  <class name="NodeValueImpl" table="NODEVALUE" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="name" column="NAME" type="string" />
    <property name="value" column="VALUE" type="string" />
    <many-to-one name="scale" column="SCALE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <many-to-one name="dataType" column="DATA_TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <many-to-one name="type" column="TYPE_ID" class="gov.nih.nci.mageom.domain.Description.impl.OntologyEntryImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

