<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.BioAssayData.impl">
  <class name="DesignElementMappingImpl" table="DESIGNELEMENTMAPPING" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <set name="designElementMaps" table="DESIGNELEMENTMAPS" lazy="true">
      <key column="DESIGN_ELEMENT_MAPPING_ID" />
      <many-to-many class="DesignElementMapImpl" column="DESIGN_ELEMENT_MAP_ID" />
    </set>
  </class>
</hibernate-mapping>

