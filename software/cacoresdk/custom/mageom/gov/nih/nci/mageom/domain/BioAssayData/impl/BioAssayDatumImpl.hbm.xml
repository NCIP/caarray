<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.BioAssayData.impl">
  <class name="BioAssayDatumImpl" table="DATUM" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="value" column="VALUE" type="string" />
    <!--
    <many-to-one name="bioAssay" column="BIOASSAY_ID" class="gov.nih.nci.mageom.domain.BioAssay.impl.BioAssayImpl" />
    <many-to-one name="designElement" column="DESIGN_ELEMENT_ID" class="gov.nih.nci.mageom.domain.DesignElement.impl.DesignElementImpl" />
    <many-to-one name="quantitationType" column="QUANTTATION_TYPE_ID" class="gov.nih.nci.mageom.domain.QuantitationType.impl.QuantitationTypeImpl" />
    -->
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

