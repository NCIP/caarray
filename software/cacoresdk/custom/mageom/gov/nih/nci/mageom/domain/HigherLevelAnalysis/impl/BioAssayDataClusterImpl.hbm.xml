<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.mageom.domain.HigherLevelAnalysis.impl">
  <class name="BioAssayDataClusterImpl" table="BIOASSAYDATACLUSTER" polymorphism="implicit">
    <id name="id" column="ID" type="long">
      <generator class="assigned"/>
    </id>
    <property name="identifier" column="IDENTIFIER" type="string" />
    <property name="name" column="NAME" type="string" />
    <set name="nodes" lazy="true">
      <key column="ID" />
      <one-to-many class="NodeImpl" />
    </set>
    <many-to-one name="clusterBioAssayData" column="CLUSTER_BIOASSAY_DATA_ID" class="gov.nih.nci.mageom.domain.BioAssayData.impl.BioAssayDataImpl" />
    <many-to-one name="security" column="DESC_SECURITY_ID" class="gov.nih.nci.mageom.domain.AuditAndSecurity.impl.SecurityImpl" />
    <set name="propertySets" lazy="true">
      <key column="ID" />
      <one-to-many class="gov.nih.nci.mageom.domain.impl.NameValueTypeImpl" />
    </set>
  </class>
</hibernate-mapping>

