       xsi:schemaLocation="http://www.jboss.com/xml/ns/javaee  
                           http://www.jboss.org/j2ee/schema/jboss_5_0.xsd"  
       version="3.0">
  <security-domain>caarray</security-domain>
  <unauthenticated-principal>__anonymous__</unauthenticated-principal>
  
  <enterprise-beans>  
