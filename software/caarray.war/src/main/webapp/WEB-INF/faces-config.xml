<?xml version='1.0' encoding='UTF-8'?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>

    <navigation-rule>
        <navigation-case>
            <from-outcome>editProtocol</from-outcome>
            <to-view-id>/protocol/editProtocol.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>searchProtocols</from-outcome>
            <to-view-id>/protocol/searchProtocols.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/protocol/editProtocol.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/protocol/viewProtocol.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/protocol/protocolError.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <managed-bean>
        <managed-bean-name>protocolBean</managed-bean-name>
        <managed-bean-class>gov.nih.nci.caarray.ui.jsf.beans.ProtocolBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>searchProtocolsBean</managed-bean-name>
        <managed-bean-class>gov.nih.nci.caarray.ui.jsf.beans.SearchProtocolsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>


</faces-config>