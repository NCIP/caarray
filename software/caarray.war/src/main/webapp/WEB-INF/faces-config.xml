<?xml version='1.0' encoding='UTF-8'?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>

    <managed-bean>
        <managed-bean-name>projectProposalBean</managed-bean-name>
        <managed-bean-class>gov.nih.nci.caarray.ui.jsf.beans.project.ProjectProposalBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>projectManagementBean</managed-bean-name>
        <managed-bean-class>gov.nih.nci.caarray.ui.jsf.beans.project.ProjectManagementBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <navigation-rule>
        <navigation-case>
            <from-outcome>startProjectProposal</from-outcome>
            <to-view-id>/project/projectProposal.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>workspace</from-outcome>
            <to-view-id>/project/workspace.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>project</from-outcome>
            <to-view-id>/project/project.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>manageFiles</from-outcome>
            <to-view-id>/project/files.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/project/projectProposal.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/project/workspace.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>